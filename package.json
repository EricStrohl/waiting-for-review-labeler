{
  "name": "nuxt-app",
  "private": true,
  "engines": {
    "node": ">=18.7.0"
  },
  "scripts": {
    "build": "nuxt build",
    "start": "nuxt build && nuxt preview",
    "dev": "nuxt dev --https",
    "generate": "nuxt generate",
    "prepare": "husky install",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint:js": "eslint --ext \".ts,.vue\" --ignore-path .eslintignore .",
    "lint:prettier": "prettier --check .",
    "lint": "yarn lint:js && yarn lint:prettier --write",
    "lint:fix": "prettier --write --list-different . && yarn lint:js --fix",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "npx playwright test",
    "storybook": "storybook dev --port 6006",
    "build-storybook": "storybook build",
    "build_action": "ncc build .github/custom_actions/$ACTION_NAME/src/$ACTION_NAME.${EXTENSION:=js} -o .github/custom_actions/$ACTION_NAME/dist"
  },
  "resolutions": {
    "string-width": "4.2.3"
  },
  "devDependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "@nuxt/devtools": "latest",
    "@nuxt/test-utils": "^3.8.0",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/storybook": "7.0.0",
    "@nuxtjs/tailwindcss": "^6.8.0",
    "@playwright/test": "^1.39.0",
    "@rollup/plugin-graphql": "^2.0.4",
    "@storybook-vue/nuxt": "0.2.1",
    "@storybook/addon-essentials": "7.5.3",
    "@storybook/addon-interactions": "7.5.3",
    "@storybook/addon-links": "7.5.3",
    "@storybook/blocks": "7.5.3",
    "@storybook/builder-vite": "7.5.3",
    "@storybook/test": "^7.6.4",
    "@storybook/testing-library": "^0.2.0",
    "@storybook/vue3": "7.5.3",
    "@storybook/vue3-vite": "^7.6.4",
    "@types/googlemaps": "^3.43.3",
    "@types/luxon": "^3.3.3",
    "@types/node": "^18.18.6",
    "@typescript-eslint/parser": "^6.8.0",
    "@vercel/ncc": "^0.38.1",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitest/coverage-v8": "^0.34.6",
    "@vue/test-utils": "^2.4.1",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-vitest": "^0.3.3",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "luxon": "^3.4.3",
    "nuxt": "^3.8.0",
    "nuxt-graphql-client": "^0.2.31",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.69.4",
    "storybook": "7.5.3",
    "typescript": "^5.2.2",
    "unplugin-auto-import": "^0.16.6",
    "vite-plugin-checker": "^0.6.2",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.25",
    "dayjs": "1.10.8"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.16",
    "@heroicons/vue": "^2.0.18",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4"
  }
}
